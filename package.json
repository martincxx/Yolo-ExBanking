    "mocha": "^10.2.0",
    "node-fetch": "^3.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "artillery": "^1.7.9",
